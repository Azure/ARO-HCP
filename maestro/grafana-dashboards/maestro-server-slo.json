              "sort": "none"
            }
          },
          "targets": [
            {
              "datasource": {
                "type": "prometheus",
                "uid": "${datasource}"
              },
              "editorMode": "code",
              "expr": "sum(rate(resources_spec_resync_duration_seconds_bucket{le=~\"^10(\\\\.0)?$\"}[5m]))\n/\nsum(rate(resources_spec_resync_duration_seconds_bucket{le=\"+Inf\"}[5m]))",
              "legendFormat": "__auto"
            }
          ],
          "title": "Maestro Server Spec Resync Duration",
          "type": "timeseries"
