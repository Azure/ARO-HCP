.create-merge table frontendContainerLogs (
    timestamp: datetime,
    log: dynamic,
    environment: string,
    cluster: string,
  	namespace: string,
    containerName: string,
    containerImage: string,
    level: string,
    msg: string,
    requestId: string,
    clientRequestId: string,
    correlationRequestId: string,
    requestMethod: string,
    requestPath: string,
    requestProto: string,
    requestQuery: string,
    requestReferer: string,
    requestRemoteAddr: string,
    requestUserAgent: string,
    bodyReadBytes: int,
    bodyWrittenBytes: int,
    responseStatusCode: string,
    duration: int,
    tag: string,
    unmatched: dynamic
)


