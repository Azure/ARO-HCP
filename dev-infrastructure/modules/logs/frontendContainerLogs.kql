.create-merge table frontendContainerLogs (
    log: dynamic,
    tag: string,
    timestamp: datetime,
  	namespace: string,
    container_image: string,
  	host: string,
    labels: string,
    podName: string,
    region: string,
    cluster: string,
    environment: string,
    unmatched: dynamic,
    level: string,
    requestId: string,
    clientRequestId: string,
    correlationRequestId: string,
    requestMethod: string,
    requestPath: string,
    requestProto: string,
    requestQuery: string,
    requestReferer: string,
    requestRemoteAddr: string,
    requestUserAgent: string,
    bodyReadBytes: int,
    bodyWrittenBytes: int,
    responseStatusCode: string,
    duration: int
)


