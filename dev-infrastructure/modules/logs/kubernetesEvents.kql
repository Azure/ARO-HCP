.create-merge table kubernetesEvents (
    log: dynamic,
    tag: string,
    timestamp: datetime,
  	namespace: string,
    container_image: string,
  	host: string,
    labels: string,
    podName: string,
    region: string,
    cluster: string,
    environment: string,
    unmatched: dynamic,
    count: int,
    kubeEventType: string,
    firstSeen: datetime,
    lastSeen: datetime,
    message: string,
    event_namespace: string,
    objectKind: string,
    objectName: string,
    objectApiVersion: string,
    objectUID: string,
    reason: string,
    sourceComponent: string
)


