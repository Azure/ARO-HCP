{
  "title": "Credential Refresher Dashboard",
  "uid": "credential-refresher",
  "panels": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "axisLabel": "Number of Credentials"
          },
          "mappings": [
            {
              "options": {
                "0": { "text": "<0" },
                "3": { "text": "0-3" },
                "7": { "text": "4-7" },
                "14": { "text": "8-14" },
                "30": { "text": "15-30" },
                "60": { "text": "31-60" },
                "Infinity": { "text": ">60" }
              },
              "type": "value"
            }
          ]
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "le"
            },
            "properties": [
              {
                "id": "custom.axisLabel",
                "value": "Days until Expiration"
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "legend": {
          "showLegend": false
        },
        "xAxis": {
          "mode": "field",
          "name": "Days until Expiration"
        },
        "xField": "le"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "expr": "rate(credential_refresher_days_until_msi_credential_expiration_bucket[60m]) * 600",
          "format": "table",
          "instant": true,
          "refId": "A"
        }
      ],
      "title": "MSI Credential Expiration Distribution",
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true,
              "instance": true,
              "job": true
            },
            "renameByName": {
              "Value": "Count"
            }
          }
        },
        {
          "id": "sortBy",
          "options": {
            "sort": [
              {
                "field": "le",
                "desc": false
              }
            ]
          }
        }
      ],
      "type": "barchart"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "id": 2,
      "options": {
        "legend": {
          "showLegend": false
        }
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "expr": "rate(credential_refresher_process_time_seconds_sum[5m]) / rate(credential_refresher_process_time_seconds_count[5m])",
          "legendFormat": "Average process time",
          "refId": "A"
        }
      ],
      "title": "Average Credential Refresher Process Time",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Error rate"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "red",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 8
      },
      "id": 3,
      "options": {},
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "expr": "rate(credential_refresher_process_time_seconds_count{outcome=\"error\"}[5m])",
          "legendFormat": "Error rate",
          "refId": "A"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "expr": "rate(credential_refresher_process_time_seconds_count[5m])",
          "legendFormat": "Total request rate",
          "refId": "B"
        }
      ],
      "title": "Credential Refresher Error vs Total Rate",
      "type": "timeseries"
    }
  ],
  "tags": [
    "credential-refresher",
    "prometheus"
  ],
  "templating": {
    "list": [
      {
        "label": "Datasource",
        "name": "datasource",
        "query": "prometheus",
        "type": "datasource"
      }
    ]
  },
  "time": {
    "from": "now-1h",
    "to": "now"
  }
}